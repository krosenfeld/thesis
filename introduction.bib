@article{akeson98,
  title = {A Circumstellar Dust Disk around T Tauri N: Subarcsecond Imaging at \ensuremath{\lambda} = 3 Millimeters},
  volume = {505},
  issn = {0004-637X},
  shorttitle = {A Circumstellar Dust Disk around T Tauri N},
  url = {http://adsabs.harvard.edu/abs/1998ApJ...505..358A},
  doi = {10.1086/306167},
  abstract = {We present high-resolution imaging of the young binary T Tauri in 
continuum emission at \ensuremath{\lambda} = 3 mm. Compact dust emission with
integrated flux density 50 +/- 6 mJy is resolved in an aperture
synthesis map at 0.5" resolution and is centered at the position of the
optically visible component, T Tau N. No emission above a 3 \ensuremath{\sigma}
level of 9 mJy is detected 0.7" south of T Tau N at the position of the
infrared companion, T Tau S. We interpret the continuum detection as
arising from a circumstellar disk around T Tau N, and estimate its
properties by fitting a flat-disk model to visibilities at \ensuremath{\lambda} = 1
and 3 mm, and to the flux density at \ensuremath{\lambda} = 7 mm. Given the data,
probability distributions are calculated for values of the free
parameters, including the temperature, density, dust opacity, and disk
outer radius. The radial variation in temperature and density is not
narrowly constrained by the data. The most likely value of the frequency
dependence of the dust opacity,
\ensuremath{\beta}=0.53+0.27-0.17, is consistent with that of
disks around other single T Tauri stars in which grain growth is
believed to have taken place. The outer radius,
R=41+26-14 AU, is smaller than the projected
separation between T Tau N and T Tau S, and may indicate tidal or
resonance truncation of the disk by T Tau S. The total mass estimated
for the disk, log (MD/M\ensuremath{\odot}) =
-2.4+0.7-0.6, is similar to masses observed around
many single pre-main-sequence sources and, within the uncertainties, is
similar to the minimum nebular mass required to form a planetary system
like our own. This observation strongly suggests that the presence of a
binary companion does not rule out the possibility of formation of a
sizable planetary system.},
  timestamp = {2015-09-03 12:27:35},
  urldate = {2015-08-27},
  journal = {The Astrophysical Journal},
  author = {Akeson, R. L. and Koerner, D. W. and Jensen, E. L. N.},
  month = sep,
  year = {1998},
  keywords = {STARS: CIRCUMSTELLAR MATTER,STARS: INDIVIDUAL: CONSTELLATION NAME: T TAURI,Stars: Pre-Main-Sequence},
  pages = {358--362},
  file = {NASA/ADS Full Text PDF:/home/krosenfe/.mozilla/firefox/5dl2iz3a.default/zotero/storage/9XJDM44H/Akeson et al. - 1998 - A Circumstellar Dust Disk around T Tauri N Subarc.pdf:application/pdf}
}

@article{dartois03,
  title = {Structure of the {DM} Tau Outer Disk: Probing the vertical kinetic temperature gradient},
  volume = {399},
  issn = {0004-6361},
  shorttitle = {Structure of the {DM} Tau Outer Disk},
  url = {http://adsabs.harvard.edu/abs/2003A\%26A...399..773D},
  doi = {10.1051/0004-6361:20021638},
  abstract = {We use high angular and spectral resolution (\{ {\textasciitilde} \} 1'' \& 0.1 km 
s-1) images in 12CO, 13CO J=2 -\ensuremath{>} 1,
J=1 -\ensuremath{>} 0 and C18O J=2 -\ensuremath{>} 1 coming from the IRAM
interferometer to probe the vertical temperature distribution in the
disk of DM Tau. We investigate here a new method based on the different
opacities of the CO isotopomers to sample the temperature disk structure
at various vertical scales inside the disk. Typically, the
12CO transitions are sampling at 2-4 scale heights, while the
13CO data are more likely tracing 1 scale height for the
J=2-1, and the disk mid-plane for the J=1-0 line. At the disk scale at
which the IRAM observations are sensitive (R\ensuremath{>}= 50-60 AU), the
analysis reveals a vertical temperature gradient. The outer layers where
tau (12CO J=2 -\ensuremath{>} 1) ={\textasciitilde} 1 are warmer (\{ {\textasciitilde} \} 30 K) than
the inner layers probed by 13CO and C18O data (\{ {\textasciitilde}
\} 13-20 K). These findings are consistent with the structure expected
for flared irradiated disks around TTauri stars. We also observe that
the outer radius of the disk is smaller in 13CO and
C18O than in 12CO. These differences in truncation
radius are in agreement with photodissociation effects. We also note
that the dynamical mass determination from CO is weakly affected by the
temperature gradient.},
  timestamp = {2015-07-10 14:56:28},
  urldate = {2015-07-07},
  journal = {Astronomy and Astrophysics},
  author = {Dartois, E. and Dutrey, A. and Guilloteau, S.},
  month = feb,
  year = {2003},
  keywords = {radio-lines: stars,STARS: CIRCUMSTELLAR MATTER,stars: individual: DM Tauri,stars: pre-main sequence},
  pages = {773--787},
  file = {NASA/ADS Full Text PDF:/home/krosenfe/.mozilla/firefox/5dl2iz3a.default/zotero/storage/DWUKAGGA/Dartois et al. - 2003 - Structure of the DM Tau Outer Disk Probing the ve.pdf:application/pdf}
}

@article{qi11,
  title = {Resolving the {CO} Snow Line in the Disk around {HD} 163296},
  volume = {740},
  issn = {0004-637X},
  url = {http://adsabs.harvard.edu/abs/2011ApJ...740...84Q},
  doi = {10.1088/0004-637X/740/2/84},
  abstract = {We report Submillimeter Array observations of CO (J = 2-1, 3-2, and 6-5) 
and its isotopologues (13CO J = 2-1, C18O J = 2-1,
and C17O J = 3-2) in the disk around the Herbig Ae star HD
163296 at {\textasciitilde}2'' (250 AU) resolution, and interpret these data in the
framework of a model that constrains the radial and vertical location of
the line emission regions. First, we develop a physically
self-consistent accretion disk model with an exponentially tapered edge
that matches the spectral energy distribution and spatially resolved
millimeter dust continuum emission. Then, we refine the vertical
structure of the model using wide range of excitation conditions sampled
by the CO lines, in particular the rarely observed J = 6-5 transition.
By fitting 13CO data in this structure, we further constrain
the vertical distribution of CO to lie between a lower boundary below
which CO freezes out onto dust grains (T \ensuremath{<}{\textasciitilde} 19 K) and an upper
boundary above which CO can be photodissociated (the hydrogen column
density from the disk surface is \ensuremath{<}{\textasciitilde} 1021 cm-2).
The freezeout at 19 K leads to a significant drop in the gas-phase CO
column density beyond a radius of {\textasciitilde}155 AU, a "CO snow line" that we
directly resolve. By fitting the abundances of all CO isotopologues, we
derive isotopic ratios of 12C/13C,
16O/18O, and 18O/17O that
are consistent with quiescent interstellar gas-phase values. This
detailed model of the HD 163296 disk demonstrates the potential of a
staged, parametric technique for constructing unified gas and dust
structure models and constraining the distribution of molecular
abundances using resolved multi-transition, multi-isotope observations.},
