  month = apr,
  year = {2009},
  keywords = {accretion,accretion disks,black hole physics,Galaxy: center,submillimeter,techniques: interferometric},
  pages = {59--74},
  file = {NASA/ADS Full Text PDF:/home/krosenfe/.mozilla/firefox/5dl2iz3a.default/zotero/storage/QDS2TZCP/Doeleman et al. - 2009 - Detecting Flaring Structures in Sagittarius A wit.pdf:application/pdf}
}

