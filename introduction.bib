  pages = {536--542}
}

@article{simon00,
  title = {Dynamical Masses of T Tauri Stars and Calibration of Pre-Main-Sequence Evolution},
  volume = {545},
  issn = {0004-637X},
  url = {http://adsabs.harvard.edu/abs/2000ApJ...545.1034S},
  doi = {10.1086/317838},
  abstract = {We have used the high sensitivity and resolution of the IRAM 
interferometer to produce subarcsecond 12CO J=2-1 images of
nine protoplanetary disks surrounding T Tauri stars in the Taurus-Auriga
cloud (seven singles and two binaries). The images demonstrate the disks
are in Keplerian rotation around their central stars. Using the
least-square fit method described in the 1998 work by Guilloteau \&
Dutrey, we derive the disk's properties, in particular its inclination
angle and rotation velocity, hence the dynamical mass. Since the disk
mass is usually small, this is a direct measurement of the stellar mass.
Typically, we reach an internal precision of 10\% in the determinations
of stellar mass. The overall accuracy is limited by the uncertainty in
the distance to a specific star. In a distance-independent way, we
compare the derived masses with theoretical tracks of pre-main-sequence
evolution. Combined with the mean distance to the Taurus region (140
pc), for stars with mass close to 1 Msolar, our results tend
to favor the tracks with cooler photospheres (higher masses for a given
spectral type). We find that in UZ Tau E, the disk and the spectroscopic
binary orbit appear to have different inclinations.},
  timestamp = {2015-08-14 14:53:15},
  urldate = {2015-07-08},
  journal = {The Astrophysical Journal},
  author = {Simon, M. and Dutrey, A. and Guilloteau, S.},
  month = dec,
  year = {2000},
  keywords = {CY Tauri,DL Tauri,DM Tauri,GG Tauri,GM Aurigae,LkCa 15,MWC 480,Radio Lines: Stars,stars: binaries: close,STARS: CIRCUMSTELLAR MATTER,stars: individual (BP Tauri,Stars: Pre-Main-Sequence,Stars: Variables: Other,UZ Tauri)},
  pages = {1034--1043},
  file = {NASA/ADS Full Text PDF:/home/krosenfe/.mozilla/firefox/5dl2iz3a.default/zotero/storage/S2BII6EV/Simon et al. - 2000 - Dynamical Masses of T Tauri Stars and Calibration .pdf:application/pdf}
}

@article{oberg11,
