  keywords = {ISM: molecules,STARS: CIRCUMSTELLAR MATTER,Stars: Planetary Systems: Protoplanetary Disks,stars: pre-main sequence},
  pages = {622--632},
  file = {NASA/ADS Full Text PDF:/home/krosenfe/.mozilla/firefox/5dl2iz3a.default/zotero/storage/D3MB4CEE/Aikawa et al. - 2002 - Warm molecular layers in protoplanetary disks.pdf:application/pdf}
}

